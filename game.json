{
	"width": 1280,
	"height": 720,
	"fps": 30,
	"main": "./script/main.js",
	"assets": {
		"player": {
			"type": "image",
			"width": 32,
			"height": 32,
			"path": "image/player.png"
		},
		"shot": {
			"type": "image",
			"width": 16,
			"height": 16,
			"path": "image/shot.png"
		},
		"se": {
			"type": "audio",
			"path": "audio/se",
			"systemId": "sound",
			"duration": 250,
			"hint": {
				"extensions": [
					".aac",
					".ogg"
				]
			}
		},
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"game-stick": {
			"type": "image",
			"path": "image/game-stick.png",
			"width": 200,
			"height": 200
		}
	},
	"environment": {
		"sandbox-runtime": "3",
		"nicolive": {
			"supportedModes": [
				"single"
			]
		}
	},
	"globalScripts": []
}